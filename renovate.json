{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "ghcr.io/umami-software/umami"
      ],
      "allowedVersions": "/^postgres-/",
      "groupName": "umami"
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "ghcr.io/berriai/litellm"
      ],
      "allowedVersions": "/^main-.*-stable$/",
      "groupName": "litellm"
    },
    {
      "matchDatasources": [
        "github-releases"
      ],
      "matchFileNames": [
        "**/kustomization.yaml",
        "**/kustomization.yml"
      ],
      "enabled": true
    }
  ],
  "kubernetes": {
    "managerFilePatterns": [
      "/^kustomization\\.ya?ml$/"
    ]
  }
}
