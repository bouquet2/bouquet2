{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^manifests/apps/umami/kustomization\\.yaml$/"
      ],
      "matchStrings": [
        "- name: (?<depName>ghcr.io/umami-software/umami)\\s+newTag: postgres-(?<currentValue>.*)"
      ],
      "datasourceTemplate": "github-tags",
      "packageNameTemplate": "umami-software/umami",
      "versioningTemplate": "semver",
      "extractVersionTemplate": "^v(?<version>.*)$",
      "depNameTemplate": "ghcr.io/umami-software/umami"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^manifests/apps/litellm/kustomization\\.yaml$/"
      ],
      "matchStrings": [
        "- name: (?<depName>ghcr.io/berriai/litellm)\\s+newTag: main-v(?<currentValue>.*)-stable"
      ],
      "datasourceTemplate": "github-tags",
      "packageNameTemplate": "berriai/litellm",
      "versioningTemplate": "semver",
      "extractVersionTemplate": "^v(?<version>.*)$",
      "depNameTemplate": "ghcr.io/berriai/litellm"
    }
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "manifests/apps/umami/kustomization.yaml"
      ],
      "matchDatasources": [
        "github-tags"
      ],
      "matchPackageNames": [
        "umami-software/umami"
      ],
      "prBodyNotes": [
        "Version will be installed as: postgres-{{{newVersion}}}"
      ]
    },
    {
      "matchFileNames": [
        "manifests/apps/litellm/kustomization.yaml"
      ],
      "matchDatasources": [
        "github-tags"
      ],
      "matchPackageNames": [
        "berriai/litellm"
      ],
      "prBodyNotes": [
        "Version will be installed as: main-{{{newVersion}}}-stable"
      ]
    }
  ]
}
