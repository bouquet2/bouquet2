{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["ghcr.io/umami-software/umami"],
      "matchVersions": ["postgres-.*"],
      "groupName": "umami"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["^ghcr.io/berriai/litellm"],
      "groupName": "litellm"
    }
  ]
}
